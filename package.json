{
  "name": "nextjs-todo-list",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "node --trace-deprecation node_modules/next/dist/bin/next dev",
    "build": "next build",
    "gen:schema": "supabase gen types --lang=typescript --local > lib/schema.ts",
    "gen:schema:remote": "supabase gen types --lang=typescript --linked remote > lib/schema.ts",
    "start": "next start",
    "lint": "next lint",
    "test": "node --trace-deprecation node_modules/vitest/vitest.mjs",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:e2e": "playwright test",
    "test:e2e:dev": "playwright test --debug",
    "prepare-merge": "supabase db diff --schema public > supabase/migrations/prepare_merge.sql",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "overrides": {
    "@supabase/node-fetch": {
      "whatwg-url": "^13"
    }
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/supabase-js": "^2.49.1",
    "@tanstack/react-query": "^5.67.2",
    "@types/node": "18.19.80",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "date-fns": "^4.1.0",
    "eslint": "8.57.1",
    "eslint-config-next": "^15.2.1",
    "lucide-react": "^0.479.0",
    "next": "^15.2.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-use-size": "^5.0.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "4.9.5",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@playwright/test": "^1.51.0",
    "@storybook/addon-essentials": "^8.6.8",
    "@storybook/addon-interactions": "^8.6.8",
    "@storybook/addon-onboarding": "^8.6.8",
    "@storybook/addon-viewport": "^8.6.8",
    "@storybook/blocks": "^8.6.8",
    "@storybook/nextjs": "^8.6.8",
    "@storybook/react": "^8.6.8",
    "@storybook/test": "^8.6.8",
    "@testing-library/jest-dom": "^6.6.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.0.8",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "eslint-plugin-storybook": "^0.11.6",
    "jsdom": "^26.0.0",
    "playwright": "^1.51.0",
    "postcss": "^8.5.3",
    "storybook": "^8.6.8",
    "supabase": "^2.19.7",
    "tailwindcss": "^3.4.17",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.8",
    "vitest-fail-on-console": "^0.7.1"
  }
}
